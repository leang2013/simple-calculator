{
  "name": "tasks",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "body-parser": "^1.19.0",
    "express": "4.17.1",
    "path": "0.12.7",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "bignumber.js": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "7.10.4",
    "@module-federation/dashboard-plugin": "^2.1.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "autoprefixer": "^10.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "8.1.0",
    "bundle-loader": "0.5.6",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^7.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-redux": "0.2.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-trx-results-processor": "^2.2.0",
    "mini-css-extract-plugin": "^1.3.4",
    "nyc": "15.1.0",
    "serve": "^11.3.2",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "4.2.0",
    "url-loader": "4.1.1",
    "webpack": "5.11.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "3.11.0"
  },
  "scripts": {
    "compiles-proto": "sh ./scripts/protoc.sh",
    "start": "cross-env NODE_ENV=development webpack serve",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production --progress",
    "lint": "eslint src/**/*.js --ignore-pattern node_modules",
    "lint:fix": "eslint src/**/*.js --fix ",
    "lint:ci": "eslint src/**/*.js --ignore-pattern node_modules --format html --output-file report/lint/report.html",
    "lintsass": "sass-lint -v",
    "lintsass:fix": "sass-lint-auto-fix -d",
    "test": "cross-env NODE_ENV=test jest --config jest-config.json",
    "test:watch": "cross-env NODE_ENV=test jest --config jest-config.json --watch --coverage=false",
    "test:coverage": "cross-env NODE_ENV=test jest --config jest-config.json --coverage",
    "test:ci": "jest --ci --config jest-config.json --coverage --reporters=default --reporters=jest-junit --reporters=jest-trx-results-processor --coverageReporters=cobertura",
    "serve": "node ./server.js",
    "clean": "rm -rf dist"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": ["json", "html"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run lint",
      "pre-push": "npm run test && npm run lint"
    }
  }
}
